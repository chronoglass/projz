{
  "name": "thing",
  "plural": "things",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "level": {
      "type": "number",
      "required": false,
      "comments": "default 0"
    },
    "shared": {
      "type": "boolean",
      "required": false,
      "comments": "default no"
    },
    "dcreated": {
      "type": "date",
      "required": true,
      "comments": "date created"
    },
    "dedited": {
      "type": "date",
      "required": true,
      "comments": "last date edited"
    },
    "desc": {
      "type": "string",
      "required": false,
      "comments": "information"
    },
    "title": {
      "type": "string",
      "required": true,
      "comments": "title of the thing"
    },
    "ownerid": {
      "type": "object",
      "required": false,
      "comments": "who created this thing, default: creater"
    },
    "parent": {
      "type": "object",
      "comments": "top level, or task toward thing"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": []
}
